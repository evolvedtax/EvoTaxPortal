@model FormRequest

<div class="tab">
    <div class="text-center"><span><p>Basic Information</p></span></div>
    <div class="form-box">
        <div class="col-md-12">
            <label>Are you a partnership, corporation, or association created or organized in the United States or under the laws of the United States including an estate or trust?</label>
            <div class="form-group">
                <div class="form-check abc-radio abc-radio-info form-check-inline">
                    <input class="form-check-input" type="radio" id="inlineRadio1" value="@AppConstants.W9Form" onclick="SelectForm(this)" asp-for="FormType">
                    <label class="form-check-label" for="inlineRadio1"> Yes </label>
                </div>
                <div class="form-check abc-radio form-check-inline">
                    <input class="form-check-input" type="radio" id="inlineRadio2" value="@AppConstants.W8FormTypes" onclick="SelectForm(this)" asp-for="FormType">
                    <label class="form-check-label" for="inlineRadio2"> No </label>
                </div>
            </div>
        </div>
    </div>
    <div id="WFormDiv" class="form-box" hidden>
        <div class="col-md-12">
            <div class="form-group">
                <label>Please Select One of the following</label>

            </div>
        </div>
        <div class="col-md-12">
            <div class="form-group">
                <div class="form-check inline">
                    <input class="form-check-input" type="radio" id="W8BENEForm1" value="@AppConstants.W8BENEForm" asp-for="W8FormType">
                    <label class="form-check-label" for="W8BENEForm1">
                        An entity claiming foreign status or claiming treaty benefits? {W-8BEN-E 'Certificate of Status of Beneficial Owner for United States Tax Withholding and Reporting (Entities)'}
                    </label>
                </div>
                <br />

                <div class="form-check inline">
                    <input class="form-check-input" type="radio" id="W8ECIForm1" value="@AppConstants.W8ECIForm" asp-for="W8FormType">
                    <label class="form-check-label" for="W8ECIForm1">
                        A payee or beneficial owner indicating that all the income is effectively connected with the conduct of a trade or business within the United States? {W-8ECI 'Certificate of Foreing Person's Claim That Income is Effectively Connected with the Conduct of a Trade or Business in the United States'}
                    </label>
                </div>
                <br />
                <div class="form-check inline">
                    <input class="form-check-input" type="radio" id="inlineRadio3" value="@AppConstants.W8IMYForm" asp-for="W8FormType">
                    <label class="form-check-label" for="inlineRadio3">
                        An intermediary, a withholding foreign partnership, a withholding foreign trust, or flow through entity? {W-8IMY 'Certificate of Foreing Intermediary, Foregin Flow-Through Entity, or certain U.S. Branches for United States Tax Withholding and Reporting'}
                    </label>

                </div>
                <br />
                <div class="form-check inline">
                    <input class="form-check-input" type="radio" id="inlineRadio4" value="@AppConstants.W8EXPForm" asp-for="W8FormType">
                    <label class="form-check-label" for="inlineRadio4">
                        A foreign government, foregin tax-exempt organization, foreign private foundation, government of a U.S. possession, or foreign central bank of issue claiming exemption from tax withholding? {W-8EXP 'Certificate of Foreign Government or Other Foreign Organization for United States Tax Withholding and Reporting'}
                    </label>
                </div>
            </div>
        </div>
    </div>
</div>
<div class="tab">
    <div class="text-center"><span><p>Entity Questions</p></span></div>
    <div class="form-box">
        <div class="row">

            <div class="col-md-6">
                <div class="form-group">
                    <label>Business name/Disregarded entity name<span class="text-danger">*</span></label>
                    <input id="GQOrgName" asp-for="GQOrgName" type="text" class="form-control" required>
                </div>
              
            </div>

               <div class="col-md-6" id="OwnerName" style="display: none;">
                <div class="form-group">
                    <label>Owner's Name<span class="text-danger">*</span></label>
                    <input id="firstName" asp-for="GQFirstName" type="text" class="form-control" required>
                </div>
            </div>
            <div class="col-md-12">
                <div class="form-group">
                    <input id="DisregardedEntity" type="checkbox" asp-for="DisregardedEntityW9"> Disregarded Entity
                </div>
            </div>

         
           

            <div id="W9Country" class="col-md-6" hidden>
                <div class="form-group">
                    <label>Country of Incorporation/Organization<span class="text-danger">*</span></label>
                    <select id="CcountryW9" asp-for="Ccountry" asp-items="ViewBag.CountriesList" required>
                        <option></option>
                    </select>
                </div>
            </div>

            <div id="SSNDiv" class="col-md-6">
                <div class="form-group">
                    <label>I.R.S Employee Identification Number (EIN)<span class="text-danger">*</span></label>
                    <input id="Ssnitnein" asp-for="Ssnitnein" type="text" placeholder="00-0000000" class="form-control" required>
                </div>
            </div>

            <div id="W9Entity" class="col-md-6">
                <div class="form-group">
                    <label>Type of Entity<span class="text-danger">*</span></label>
                    <select id="EntityTypeW9" asp-for="EntityType" asp-items="ViewBag.EntitiesList" required>
                        <option></option>
                    </select>
                </div>

            </div>



            <div class="col-md-6" id="OherId" style="display: none;">
                <div class="form-group">
                    <label>Other-Text<span class="text-danger">*</span></label>
                    <input id="other" asp-for="OtherEntityType" type="text" class="form-control">
                </div>
            </div>




            <div class="col-md-12" style="display: none;" id="chkbox3">
                <div class="form-group">

                    @*   <div class="mt-3 mb-3 ml-3"> *@
                    <input id="chkbox3-text" type="checkbox" asp-for="ThreeB" class="">
                      <label id="chkbox3-label" for="s">Please check this box if you have any foreign individual in this entity.</label>
                </div>
            </div>

        </div>
    </div>


</div>

<div class="tab">
    <div class="text-center"><span><p>Mailing Address</p></span></div>
    <div class="form-box">
        <div class="row">
            <div class="col-md-6">
                <div class="form-group">
                    <label>Country <span class="text-danger">*</span></label>
                    <select id="country" asp-for="MCountry" asp-items="ViewBag.CountriesList" onChange="ShowHideProvince(this)" class="select2" required>
                        <option></option>
                    </select>
                </div>
            </div>
            <div class="col-md-6">
                <div class="form-group">
                    <label>Address Line 1 <span class="text-danger">*</span></label> <b>(Do not use a P.O. box or in-care-of address)</b>
                    <input id="adreess1" asp-for="MAddress1" type="text" class="form-control" required>
                    <span asp-validation-for="MAddress1" class="text-danger"></span>
                </div>
            </div>
            <div class="col-md-6">
                <div class="form-group">
                    <label>Address Line 2</label>
                    <input id="adreess2" asp-for="MAddress2" type="text" class="form-control">
                </div>
            </div>

            <div class="col-md-6">
                <div class="form-group">
                    <label>City <span class="text-danger">*</span></label>
                    <input id="city" asp-for="MCity" type="text" class="form-control" required>
                </div>
            </div>
            <div class="col-md-6">
                <div id="stateDiv" class="form-group" hidden>
                    <label>State </label>
                    <select id="state" asp-items="ViewBag.StatesList" asp-for="MState" class="select2">
                        <option></option>
                    </select>
                </div>
                <div id="provinceDiv" class="form-group">
                    <label>Province</label>
                    <input id="province" asp-for="MProvince" type="text" class="form-control">
                </div>
            </div>

            <div class="col-md-6">
                <div class="form-group">
                    <label id="lblZipCode">Zip/Postal Code <span class="text-danger"> *</span></label>
                    <input id="zipCode" asp-for="MZipCode" type="text" class="form-control" required>
                </div>
            </div>
        </div>
    </div>
    <div class="mt-3 mb-3 ml-3">
        <input id="SamePermAddress" type="checkbox" checked onChange="HideAddress(this)" name="SamePermAddress" class=""> Permanent address is the same as the mailing address
    </div>
    <div id="SamePermAddressDiv" class="form-box" hidden>
        <div class="row">
            <div class="col-md-6">
                <div class="form-group">
                    <label>Country <span class="text-danger">*</span></label>
                    <select id="countryPerm" asp-for="PCountry" asp-items="ViewBag.CountriesList" onChange="ShowHideProvincePerm(this)" class="select2" required>
                        <option></option>
                    </select>
                </div>
            </div>
            <div class="col-md-6">
                <div class="form-group">
                    <label>Address Line 1 <span class="text-danger">*</span></label> <b>(Do not use a P.O. box or in-care-of address)</b>
                    <input id="adreess1Perm" asp-for="PAddress1" type="text" class="form-control" required>
                    <span asp-validation-for="PAddress1" class="text-danger"></span>
                </div>
            </div>
            <div class="col-md-6">
                <div class="form-group">
                    <label>Address Line 2</label>
                    <input id="adreess2Perm" asp-for="PAddress2" type="text" class="form-control">
                </div>
            </div>

            <div class="col-md-6">
                <div class="form-group">
                    <label>City <span class="text-danger">*</span></label>
                    <input id="cityPerm" asp-for="PCity" type="text" class="form-control" required>
                </div>
            </div>
            <div class="col-md-6">
                <div id="statePermDiv" class="form-group" hidden>
                    <label>State </label>
                    <select id="statePerm" asp-items="ViewBag.StatesList" asp-for="PState" class="select2">
                        <option></option>
                    </select>
                </div>
                <div id="provincePermDiv" class="form-group">
                    <label>Province</label>
                    <input id="provincePerm" name="provincePerm" type="text" class="form-control">
                </div>
            </div>

            <div class="col-md-6">
                <div class="form-group">
                    <label id="lblZipCodePerm">Zip/Postal Code <span class="text-danger"> *</span></label>
                    <input id="zipCodePerm" asp-for="PZipCode" type="text" class="form-control" required>
                </div>
            </div>
        </div>
    </div>
</div>
<div class="tab">
    <div class="text-center"><span><p>Additional Information</p></span></div>
    <div class="form-box">

        <div class="col-md-6">
            <div class="form-group" id="DeW8ECIEntityOwnerNameDiv" hidden>
                <label>Name of disregarded entity receiving the payments (if applicable)</label>
                <input id="DEOwnerNameW8ECI" asp-for="DEOwnerNameW8ECI" type="text" class="form-control">
            </div>
        </div>

        <div class="row">
            <div id="AuthSignatoryNameDiv" class="col-md-6" hidden>
                <div class="form-group">
                    <label>Please enter the name of the authorized signatory<span class="text-danger"> *</span></label>
                    <input asp-for="AuthSignatoryName" class="form-control" required />
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-md-12" id="BackupWithHoldingMainDiv" hidden>
                <label>Are you required to have backup withholding? If No, please select below.</label>
                <div class="form-group">
                    <div class="form-check abc-radio abc-radio-info form-check-inline">
                        <input class="form-check-input" type="radio" id="BackupWithHolding1" value="true" checked onclick="BackupWithHoldingFunc(this)" asp-for="BackupWithHolding" required>
                        <label class="form-check-label" for="BackupWithHolding1"> Yes </label>
                    </div>
                    <div class="form-check abc-radio form-check-inline">
                        <input class="form-check-input" type="radio" id="BackupWithHolding2" value="false" onclick="BackupWithHoldingFunc(this)" asp-for="BackupWithHolding" checked="checked" required>
                        <label class="form-check-label" for="BackupWithHolding2"> No </label>
                    </div>
                </div>
            </div>
            <div class="col-md-12" id="BackupWithHoldingDiv" style="padding-left: inherit;padding-right: inherit" hidden>
                <div class="col-md-12">
                    <div class="form-group">
                        <label>Please select exempt payee code from the list below<span class="text-danger">*</span></label>
                        <select id="Payeecode" asp-for="Payeecode" asp-items="ViewBag.PayeeCodeList" class="select2" required>
                            <option></option>
                        </select>
                    </div>
                </div>
                <div class="col-md-12">
                    <div class="form-group">
                        <label>Please select the exemption code from FATCA reporting<span class="text-danger">*</span></label>
                        <select id="Fatca" asp-for="W9Fatca" asp-items="ViewBag.FATCACodeList" class="select2" required>
                            <option></option>
                        </select>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>


<style>
    .form-check {
        margin-bottom: 10px; /* Adjust the margin as per your preference */
    }
</style>

