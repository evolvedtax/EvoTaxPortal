@model InstituteEntityViewModel
@{
    ViewData["Title"] = "Entity";
    Layout = "~/Views/Shared/_LayoutInstitute.cshtml";
}
<style>
    .dropdown-menu-horizontal {
        display: flex;
        flex-direction: row;
        flex-wrap: wrap;
    }

        .dropdown-menu-horizontal .dropdown-item {
            white-space: nowrap;
            margin-right: 10px;
        }

    .center-dropdown {
        left: -650% !important;
        transform: translateX(-50%) !important;
    }

    .hide-counter::-webkit-inner-spin-button {
        display: none;
    }

    .dropdown-toggle::after {
        display: inline-block;
        margin-left: 0.255em;
        vertical-align: 0.255em;
        content: none;
        border-top: 0.3em solid;
        border-right: 0.3em solid transparent;
        border-bottom: 0;
        border-left: 0.3em solid transparent;
    }

    .dataTables_filter {
        /*position: absolute;
                    top: 0;
                    left: 15%;*/
        padding: 0;
        padding-right: 70%
    }
</style>
<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox ">
                <div class="ibox-title">
                    <div class="row">
                        <div class="col-md-8">
                        </div>
                        <div id="uploadDataDiv" class="col-md-4" hidden>
                            <label>Upload Data</label>
                            <div class="input-group">
                                <div class="custom-file">
                                    <input id="uploadEntities" type="file" class="custom-file-input" accept=".xlsx, .xls">
                                    <label class="custom-file-label" for="uploadEntities">Choose file</label>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="ibox-content">
                    @*<div class="col-md-12"><input id="importData" type="button" class="btn btn-primary" value="Import Entity Data" /></div>*@
                    <div class="table-responsive">
                        <div class="form-group" style="margin-bottom:-2rem">
                            <label for="pageLengthInput">Show</label>
                            <input type="text" id="pageLengthInput" value="25" class="form-control" style="width: 50px; display: inline-block;">
                            <label for="pageLengthInput">entries</label>
                        </div>
                        <table class="table table-striped table-bordered table-hover dataTables-example">
                            <thead>
                                <tr>
                                    <th><input type="checkbox" id="selectAll"></th>
                                    <th>Entity Name</th>
                                    <th>EIN</th>
                                    <th>Entity Registration Date</th>
                                    <th>Address</th>
                                    <th hidden></th>
                                    <th hidden></th>
                                    <th>City</th>
                                    <th>State</th>
                                    <th hidden>Province (If Applicable)</th>
                                    <th>Zip</th>
                                    <th>Country of Incorporation</th>
                                    <th>Last Updated By</th>
                                    <th>Last Updated On</th>
                                    <th>Lock/Unlock</th>
                                    <th>Action</th>
                                </tr>
                            </thead>
                            <tbody>
                                @if (Model.InstituteEntitiesResponse.Any())
                                {
                                    foreach (var item in Model.InstituteEntitiesResponse)
                                    {
                                        <tr id="@item.EntityId">
                                            <td><input type="checkbox" class="rowCheckbox" value="@item.EntityId"></td>
                                            <td>@Html.ActionLink(item.EntityName,"Client","Institute",new{EntityId = item.EntityId, InstituteId = item.InstituteId}) </td>
                                            <td>@item.Ein</td>
                                            <td>@item.EntityRegistrationDate?.ToString("MM-dd-yyyy")</td>
                                            <td>@string.Concat(item.Address1," ",item.Address2)</td>
                                            <td hidden>@item.Address1</td>
                                            <td hidden>@item.Address2</td>
                                            <td>@item.City</td>
                                            <td>@item.State</td>
                                            <td hidden>@item.Province</td>
                                            <td>@item.Zip</td>
                                            <td>@item.Country</td>
                                            <td>@item.LastUpdatedByName</td>
                                            <td>@item.LastUpdatedDate?.ToString("MM-dd-yyyy")</td>
                                            <td class="text-center">
                                                @if (item?.IsLocked ?? false)
                                                {
                                                    <span class="fa fa-lock"></span>
                                                }
                                                else
                                                {
                                                    <span class="fa fa-unlock"></span>
                                                }
                                            </td>
                                            <td class="actions">
                                                <div class="dropdown">
                                                    <button class="dropdown-toggle" type="button" id="dropdownMenuButton" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                                        <span class="fa fa-bars"></span>
                                                    </button>
                                                    <div class="dropdown-menu" aria-labelledby="dropdownMenuButton">
                                                        <div class="dropdown-menu-horizontal">
                                                            @if (item?.IsLocked ?? false)
                                                            {
                                                                @*<a class="dropdown-item" href="javascript:void()" onclick="COMMON.confirmAlertActive('Unlock','LockUnlockEntity',{selectedValues:'@item?.EntityId',isLocked:'false'})">Unlock</a>*@
                                                            }
                                                            else
                                                            {
                                                                <a class="dropdown-item edit-row" href="javascript:void()" data-toggle="modal" data-target="#editModal" data-row-id="@item?.EntityId">Edit</a>
                                                                <a class="dropdown-item" href="javascript:void()" onclick="COMMON.confirmAlert('Are you sure, you want to delete this record?','DeleteEntity','id=@item?.EntityId','')">Delete</a>
                                                                @*<a class="dropdown-item" href="javascript:void()" onclick="COMMON.confirmAlertActive('Lock','LockUnlockEntity',{selectedValues:'@item?.EntityId',isLocked:'true'})">Lock</a>*@
                                                            }
                                                        </div>
                                                    </div>
                                                </div>
                                            </td>
                                        </tr>
                                    }
                                }
                            </tbody>
                        </table>
                    </div>

                </div>
                <div id="dupplication-ibox" class="ibox-content" hidden>
                    <h3>Duplicate Records</h3>
                    <div class="table-responsive">
                        <table id="entity-table-dupplication" class="table table-striped table-bordered table-hover">
                            <thead>
                                <tr>
                                    <th>Entity Name</th>
                                    <th>EIN</th>
                                    <th>Entity Registration Date</th>
                                    <th>Address</th>
                                    <th>City</th>
                                    <th>State</th>
                                    <th>Province (If Applicable)</th>
                                    <th>Zip</th>
                                    <th>Country of Incorporation</th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr>
                                    <td></td>
                                    <td></td>
                                    <td></td>
                                    <td></td>
                                    <td></td>
                                    <td></td>
                                    <td></td>
                                    <td></td>
                                    <td></td>
                                </tr>
                            </tbody>
                        </table>
                    </div>

                </div>
            </div>
        </div>
    </div>
</div>
<!--Add Modal -->
<div class="modal inmodal" id="addModal" tabindex="-1" role="dialog" aria-hidden="true">
    <div class="modal-dialog modal-lg">
        <div class="modal-content animated fadeIn">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
                <h4 class="modal-title">Add New Entity</h4>
            </div>
            <div class="modal-body">
                <form id="addForm"
                      method="post"
                      data-ajax="true"
                      data-ajax-method="post"
                      data-ajax-mode="replace"
                      data-ajax-failure="COMMON.ajaxfailure"
                      data-ajax-loading="#ajax-loader"
                      data-ajax-success="ajaxsuccessAdd"
                      data-ajax-url="@Url.Action("AddEntity","Institute")">
                    <input id="InstituteId" asp-for="InstituteEntityRequest.InstituteId" value="@ViewBag.InstituteId" hidden />
                    <div class="row col-md-12">
                        <div class="col-md-6">
                            <div class="form-group">
                                <label for="enitytName">Entity Name<span class="text-danger">*</span></label>
                                <input type="text" class="form-control" asp-for="InstituteEntityRequest.EntityName" required>
                                <span asp-validation-for="InstituteEntityRequest.EntityName" class="text-danger"></span>
                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="form-group">
                                <label for="ein">EIN<span class="text-danger">*</span></label>
                                <input type="text" class="form-control ein" asp-for="InstituteEntityRequest.Ein" required>
                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="form-group">
                                <label for="entityRegistrationDate">Entity Registration Date<span class="text-danger">*</span></label>
                                <input type="date" class="form-control" asp-for="InstituteEntityRequest.EntityRegistrationDate" required>
                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="form-group">
                                <label for="country">Country<span class="text-danger">*</span></label>
                                <select asp-items="ViewBag.CountriesList" asp-for="InstituteEntityRequest.Country" onChange="ShowHideProvince(this)" required>
                                    <option></option>
                                </select>
                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="form-group">
                                <label for="address1">Address 1<span class="text-danger">*</span></label>
                                <input type="text" class="form-control" asp-for="InstituteEntityRequest.Address1" required>
                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="form-group">
                                <label for="address2">Address 2</label>
                                <input type="text" class="form-control" asp-for="InstituteEntityRequest.Address2">
                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="form-group">
                                <label for="city">City<span class="text-danger">*</span></label>
                                <input type="text" class="form-control" asp-for="InstituteEntityRequest.City" required>
                            </div>
                        </div>
                        <div id="stateDiv" class="col-md-6 stateDiv">
                            <div class="form-group">
                                <label for="state">State</label>
                                <select class="state" asp-items="ViewBag.StatesList" asp-for="InstituteEntityRequest.State">
                                    <option></option>
                                </select>
                            </div>
                        </div>
                        <div id="provinceDiv" class="col-md-6 provinceDiv" hidden>
                            <div class="form-group">
                                <label for="province">Province</label>
                                <input type="text" class="form-control province" asp-for="InstituteEntityRequest.Province" required>
                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="form-group">
                                <label id="lblZipCode" class="lblZipCode" for="zip">Zip</label>
                                <input type="text" class="form-control" asp-for="InstituteEntityRequest.Zip" required>
                            </div>
                        </div>
                    </div>
                </form>
                <div class="modal-footer">
                    <button type="button" class="btn btn-white" data-dismiss="modal">Close</button>
                    <button type="button" class="btn btn-primary" id="saveAddBtn">Save changes</button>
                </div>
            </div>
        </div>
    </div>
</div>
<!--Edit Modal -->
<div class="modal inmodal" id="editModal" tabindex="-1" role="dialog" aria-hidden="true">
    <div class="modal-dialog modal-lg">
        <div class="modal-content animated fadeIn">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
                <h4 class="modal-title">Edit Entity</h4>
            </div>
            <div class="modal-body">
                <!-- Form for editing row data -->
                <form id="regForm"
                      method="post"
                      data-ajax="true"
                      data-ajax-method="post"
                      data-ajax-mode="replace"
                      data-ajax-failure="COMMON.ajaxfailure"
                      data-ajax-loading="#ajax-loader"
                      data-ajax-success="ajaxsuccess"
                      data-ajax-url="@Url.Action("UpdateEntity","Institute")">
                    <!-- Add input fields here for each column you want to edit -->
                    <input id="entityId" asp-for="InstituteEntityRequest.EntityId" hidden />
                    <div class="row col-md-12">
                        <div class="col-md-6">
                            <div class="form-group">
                                <label for="enitytName">Entity Name<span class="text-danger">*</span></label>
                                <div style="display:flex">
                                    <input type="text" class="form-control" id="enitytName" asp-for="InstituteEntityRequest.EntityName" readonly required>
                                    <button type="button" id="editEntityName" class="btn btn-white"><span class="fa fa-edit"></span></button>
                                </div>
                                <span asp-validation-for="InstituteEntityRequest.EntityName" class="text-danger"></span>
                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="form-group">
                                <label for="ein">EIN<span class="text-danger">*</span></label>
                                <div style="display:flex">
                                    <input type="text" class="form-control ein" id="ein" asp-for="InstituteEntityRequest.Ein" readonly required>
                                    <button type="button" id="editEIN" class="btn btn-white"><span class="fa fa-edit"></span></button>
                                </div>
                                <span asp-validation-for="InstituteEntityRequest.Ein" class="text-danger"></span>
                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="form-group">
                                <label for="entityRegistrationDate">Entity Registration Date<span class="text-danger">*</span></label>
                                <input type="date" class="form-control" id="entityRegistrationDate" asp-for="InstituteEntityRequest.EntityRegistrationDate" required>
                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="form-group">
                                <label for="country">Country<span class="text-danger">*</span></label>
                                <select id="country" asp-items="ViewBag.CountriesList" asp-for="InstituteEntityRequest.Country" onChange="ShowHideProvince(this)" required>
                                    <option></option>
                                </select>
                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="form-group">
                                <label for="address1">Address 1<span class="text-danger">*</span></label>
                                <input type="text" class="form-control" id="address1" asp-for="InstituteEntityRequest.Address1" required>
                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="form-group">
                                <label for="address2">Address 2</label>
                                <input type="text" class="form-control" id="address2" asp-for="InstituteEntityRequest.Address2">
                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="form-group">
                                <label for="city">City<span class="text-danger">*</span></label>
                                <input type="text" class="form-control" id="city" asp-for="InstituteEntityRequest.City" required>
                            </div>
                        </div>
                        <div id="stateDiv" class="col-md-6 stateDiv">
                            <div class="form-group">
                                <label for="state">State</label>
                                <select id="state" class="state" asp-items="ViewBag.StatesList" asp-for="InstituteEntityRequest.State">
                                    <option></option>
                                </select>
                            </div>
                        </div>
                        <div id="provinceDiv" class="col-md-6 provinceDiv" hidden>
                            <div class="form-group">
                                <label for="province">Province</label>
                                <input type="text" class="form-control province" id="province" asp-for="InstituteEntityRequest.Province" required>
                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="form-group">
                                <label id="lblZipCode" class="lblZipCode" for="zip">Zip</label>
                                <input type="text" class="form-control zip" id="zip" asp-for="InstituteEntityRequest.Zip" required>
                            </div>
                        </div>
                    </div>
                </form>
                <div class="modal-footer">
                    <button type="button" class="btn btn-white" data-dismiss="modal">Close</button>
                    <button type="button" class="btn btn-primary" id="saveBtn">Save changes</button>
                </div>
            </div>
        </div>
    </div>
</div>
@section Scripts {

    <script type="text/javascript">

        // Upgrade button class name
        $.fn.dataTable.Buttons.defaults.dom.button.className = 'btn btn-white btn-sm';
        $(document).ready(function () {
            $("select").select2({
                theme: 'bootstrap4',
                placeholder: "Select One",
                //allowClear: true
            });
            //Masking
            $('.ein').attr('placeholder', '00-0000000');
            $('.ein').mask('00-0000000', { reverse: true });
            // Handle Edit button click event
            $('.edit-row').click(function () {
                // Get the row ID from the data attribute
                var rowId = $(this).data('row-id');

                // Retrieve the row data based on the ID
                var rowData = getRowData(rowId); // Implement your own function to retrieve the row data

                // Populate the form fields with the row data
                populateModalFields(rowData);
            });

            function getRowData(rowId) {
                var row = $("#" + rowId); // Select the row by its ID
                var rowData = {
                    EntityId: rowId,
                    EntityName: row.find("td:nth-child(2)").text(),
                    Ein: row.find("td:nth-child(3)").text(),
                    EntityRegistrationDate: row.find("td:nth-child(4)").text(),
                    Address1: row.find("td:nth-child(6)").text(),
                    Address2: row.find("td:nth-child(7)").text(),
                    City: row.find("td:nth-child(8)").text(),
                    State: row.find("td:nth-child(9)").text(),
                    Province: row.find("td:nth-child(10)").text(),
                    Zip: row.find("td:nth-child(11)").text(),
                    Country: row.find("td:nth-child(12)").text(),
                };

                return rowData;
            }
            // Function to populate modal input fields with row data
            function populateModalFields(rowData) {
                debugger
                var parts = rowData.EntityRegistrationDate.split('-');
                $('#entityId').val(rowData.EntityId);
                $('#enitytName').val(rowData.EntityName);
                $('#ein').val(rowData.Ein);
                $('#entityRegistrationDate').val(parts[2] + '-' + parts[0] + '-' + parts[1]);
                $('#address1').val(rowData.Address1);
                $('#address2').val(rowData.Address2);
                $('#city').val(rowData.City);
                $('#country').val(rowData.Country).change();
                $('#state').val(rowData.State).change();
                $('#province').val(rowData.Province);
                $('#zip').val(rowData.Zip);
            }
            var table = $('.dataTables-example').DataTable({
                pageLength: 25,
                dom: '<"html5buttons"B>lTfgitp',
                buttons: [
                    {
                        extend: 'collection',
                        text: '<i class="fa fa-ellipsis-h"></i>',
                        className: 'btn-menu-toggle',
                        buttons: [
                            {
                                text: '<i class="fa fa-download"></i> Download New Entity Template',
                                className: 'btn-template',
                                action: function (e, dt, button, config) {
                                    let url = 'DownloadExcel';
                                    let params = { fileType: '@AppConstants.Entity' }
                                    COMMON.doAjaxToDownloadFile(url, params, '@AppConstants.InstituteEntityTemplate')
                                }
                            },
                            {
                                extend: 'excel',
                                title: 'EvolvedTax',
                                text: '<i class="fa fa-file-excel-o"></i> Export to Excel',
                                className: 'btn-excel'
                            },
                            {
                                text: '<i class="fa fa-upload"></i> Import Entity Data',
                                className: 'btn-imp-data',
                                action: function (e, dt, button, config) {
                                    $('#uploadDataDiv').prop('hidden', false);
                                }
                            },
                            {
                                text: '<i class="fa fa-trash"></i> Trash',
                                className: 'btn-recycle-bin',
                                action: function (e, dt, button, config) {
                                    window.location.href = '/Institute/EntitiesRecyleBin';
                                }
                            },
                            {
                                text: '<i class= "fa fa-unlock" ></i> Unlock All',
                                className: 'btn-unlock-all',
                                action: function (e, dt, button, config) {
                                    var selectedValues = [];
                                    $('.rowCheckbox:checked').each(function () {
                                        selectedValues.push($(this).val());
                                    });

                                    if (selectedValues.length === 0) {
                                        COMMON.notification(3, 'No row selected. Please select at least one row.');
                                        return;
                                    }

                                    //$('.loading').show();
                                    // Send the selectedValues to the controller using AJAX or a form submission
                                    COMMON.confirmAlertActiveAll('Unlock', 'LockUnlockEntity', { selectedValues: selectedValues, isLocked: 'false' })
                                }
                            },
                            {
                                text: '<i class= "fa fa-lock" ></i> Lock All',
                                className: 'btn-lock-all',
                                action: function (e, dt, button, config) {
                                    var selectedValues = [];
                                    $('.rowCheckbox:checked').each(function () {
                                        selectedValues.push($(this).val());
                                    });

                                    if (selectedValues.length === 0) {
                                        COMMON.notification(3, 'No row selected. Please select at least one row.');
                                        return;
                                    }

                                    //$('.loading').show();
                                    // Send the selectedValues to the controller using AJAX or a form submission
                                    COMMON.confirmAlertActiveAll('Lock', 'LockUnlockEntity', { selectedValues: selectedValues, isLocked: 'true' })
                                }
                            }
                        ],
                    },
                    {
                        text: 'New <i class="fa fa-plus"></i>',
                        className: 'new-btn',
                        action: function (e, dt, button, config) {
                            $("#addModal").modal("show");
                        }
                    },
                ],
                "drawCallback": function () {
                    var pageInfo = this.api().page.info();
                    $('#pageLengthInput').val(pageInfo.length);
                },
                initComplete: function () {

                    var api = this.api();
                    $('#pageLengthInput').on('keyup change', function () {
                        var value = parseInt($(this).val(), 10);
                        if (!isNaN(value)) {
                            api.page.len(value).draw();
                        }
                    });
                    $('.dataTables_length').hide(); // Hide the original page length dropdown
                    $('.btn-menu-toggle').attr('data-toggle', 'tooltip').attr('data-placement', 'top').attr('data-original-title', 'Menu').tooltip();
                    // Add custom CSS class to the dropdown list container
                    $('.btn-menu-toggle').on('click', function () {
                        var dropdown = $(this).siblings('.dt-button-collection');

                        dropdown.addClass('center-dropdown');
                    });
                    //this.api()
                    //    .columns([1,2])
                    //    .every(function () {
                    //        let column = this;
                    //        let title = column.header().textContent;

                    //        // Create input element
                    //        let input = document.createElement('input');
                    //        input.placeholder = title;
                    //        column.header().replaceChildren(input);

                    //        // Event listener for user input
                    //        input.addEventListener('keyup', () => {
                    //            if (column.search() !== input.value) {
                    //                column.search(input.value).draw();
                    //            }
                    //        });

                    //        // Event listener for click on the input element
                    //        input.addEventListener('click', (event) => {
                    //            event.stopPropagation();
                    //        });
                    //    });
                }
            });
            // Get the file input element
            var fileInput = document.getElementById('uploadEntities');
            // Add an event listener to the file input
            fileInput.addEventListener('change', uploadData);
            function uploadData(event) {
                var InstituteId = $('#InstituteId').val();
                COMMON.uploadFile(event, '/Institute/uploadEntities', InstituteId);
            }
            bsCustomFileInput.init();
        });
        // Select All checkbox
        $('#selectAll').change(function () {
            $('.rowCheckbox:not(:disabled)').prop('checked', $(this).prop('checked'));
            //$('.email-btn').text('Send Email to All');
            var selectedValues = [];
            $('.rowCheckbox:checked').each(function () {
                selectedValues.push($(this).val());
            });
            if (selectedValues.length === 0) {
                //$('.email-btn').text('Send Email');
            }
        });

        // Individual row checkbox
        $('.rowCheckbox').change(function () {
            if ($('.rowCheckbox:checked').length == $('.rowCheckbox').length) {
                $('#selectAll').prop('checked', true);
                //$('.email-btn').text('Send Email to All');
            } else {
                $('#selectAll').prop('checked', false);
                //$('.email-btn').text('Send Email to Selected');
                var selectedValues = [];
                $('.rowCheckbox:checked').each(function () {
                    selectedValues.push($(this).val());
                });
                if (selectedValues.length === 0) {
                    //$('.email-btn').text('Send Email');
                }
            }
        });
        $('#saveBtn').click(function () {
            let form = $('#regForm');
            if (form.valid()) {
                $('#enitytName').prop('disabled', false);
                $('#ein').prop('disabled', false);
                form.submit();
            }
            return false;
        });
        function ajaxsuccess(response) {
            if (response.Status == true) {
                COMMON.notification(1, "Data is updated");
            } else {
                COMMON.notification(2, "");
            }
            $("#editModal").modal("hide");
            window.location.reload();
        }
        $('#saveAddBtn').click(function () {
            let form = $('#addForm');
            if (form.valid()) {
                form.submit();
            }
            return false;
        });
        $('#editEntityName').click(function () {
            $('#enitytName').prop('readonly', false);
        });
        $('#editEIN').click(function () {
            $('#ein').prop('readonly', false);
        });
        function ajaxsuccessAdd(response) {
            if (response.Status == true) {
                COMMON.notification(1, response.Message);
                $("#addModal").modal("hide");
                window.location.reload();
            } else {
                COMMON.notification(2, response.Message);
            }

        }
        function ShowHideProvince(obj) {
            if (obj.value == 'United States') {
                $('.provinceDiv').prop('hidden', true);
                $('.province').val('');
                $('.stateDiv').prop('hidden', false);
                $('.lblZipCode').text('Zip Code').append(`<span class="text-danger"> *</span>`);
            } else {
                $('.provinceDiv').prop('hidden', false);
                $('.stateDiv').prop('hidden', true);
                $('.state').val(1);
                $('.lblZipCode').text('Zip/Postal Code').append(`<span class="text-danger"> *</span>`);
            }
        }

        $('#importData').click(function () {
            $('#uploadDataDiv').prop('hidden', false);
        });
    </script>
}